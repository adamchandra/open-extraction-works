{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts"
        ]
    },
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "trailing-comma": [true, {"multiline": "ignore", "singleline": "never"}],
        "no-consecutive-blank-lines": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
