{
  "name": "sharedLib",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "rbush": "^2.0.1",
    "rbush-knn": "^2.1.0",
    "watrmarks.js": "^0.1.0",
    "split.js": "^1.5.10"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/jasmine": "^2.8.12",
    "@types/jest": "^23.3.10",
    "@types/lunr": "^0.1.0",
    "chai": "^4.1.2",
    "jasmine": "^3.3.0",
    "jasmine-core": "^3.3.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1"
  },
  "scripts": {
    "build:prod": "tsc",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
