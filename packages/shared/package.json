{
  "name": "sharedLib",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "rbush": "^2.0.1",
    "rbush-knn": "^2.1.0",
    "watrmarksJs": "^0.1.0",
    "split.js": "^1.5.10",
    "d3": "^4.13.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/jest": "^24.0.9",
    "@types/lunr": "^0.1.0",
    "chai": "^4.1.2",
    "jest": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "build:prod": "tsc",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": {
          "ignoreCodes": [
            151001,
            7016
          ]
        }
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
