{
  "name": "sharedLib",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "d3": "^5.14.2",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "rbush": "^3.0.1",
    "rbush-knn": "^2.1.0",
    "split.js": "^1.5.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/chai": "^4.2.6",
    "@types/d3": "^5.7.2",
    "@types/jest": "^24.0.23",
    "@types/jquery": "^3.3.31",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "eslint": "^6.7.2",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "rimraf": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly --declaration",
    "build:types:watch": "npm run build:types -- --watch",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build:js:watch": "npm run build:js -- --watch",
    "build": "npm-run-all clean build:types build:js",
    "build:watch": "npm-run-all build:types:watch build:js:watch"
  },
  "main": "src/index.ts",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": {
          "ignoreCodes": [
            151001,
            7016
          ]
        }
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
