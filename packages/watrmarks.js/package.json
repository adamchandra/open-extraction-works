{
  "name": "watrmarks.js",
  "version": "0.1.0",
  "private": true,
  "bundleDependencies": false,
  "deprecated": false,
  "description": "NPM package for WatrMarks",
  "main": "./src/index.ts",
  "types": "./src/watrmarks.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && mkdir lib",
    "fetchlib": "cp ../../../watr-marks/js/target/scala-2.12/watrmarks-fastopt.js* lib/",
    "dist": "npm run clean && npm run fetchlib",
    "test": "jest"
  },
  "files": [
    "src/watrmarks.d.ts",
    "src/watrmarks-fastopt.js",
    "lib/watrmarks-fastopt.js.map",
    "src/index.ts"
  ],
  "keywords": [
    "scala",
    "WatrWorks",
    "WatrMarks"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.1"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js)$"
  }
}
