{
  "name": "watrmarksJs",
  "version": "0.1.0",
  "private": true,
  "bundleDependencies": false,
  "deprecated": false,
  "description": "NPM package for WatrMarks",
  "main": "./dist/index.js",
  "types": "./dist/watrmarks.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist && mkdir dist && rimraf lib.d && mkdir lib.d",
    "fetchlib": "cp ../../../watr-marks/js/target/scala-2.12/watrmarks-fastopt.js* lib.d/",
    "cplib": "cp lib.d/watrmarks-fastopt.js.map dist/ && cp src/watrmarks.d.ts dist/",
    "dist": "npm-run-all clean fetchlib build cplib",
    "test": "jest"
  },
  "include": [
    "src/**/*"
  ],
  "keywords": [
    "scala",
    "WatrWorks",
    "WatrMarks"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "babel-preset-env": "^1.7.0",
    "jest": "^24.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.4000"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js)$"
  }
}
