{
  "name": "watr-vue-components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --mode development ./src/main.ts",
    "watch": "vue-cli-service build --watch --mode development ./src/main.ts",
    "test": "vue-cli-service test:unit --timeout 10000 'src/**/*.test.ts'",
    "serve:json": "json-server --port 3100 ./dev-data/db.json --static ./dev-data --routes ./dev-data/routes.json",
    "serve:dev": "ts-node src/server.ts --corpus ../../../datasets.d/corpus-test.d --public ../vue-components/dist",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "develop": "npm-run-all -p -r storybook serve:dev"
  },
  "dependencies": {
    "auth0-js": "^9.10.1",
    "d3": "^4.13.0",
    "events": "^3.0.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "lunr": "^2.3.6",
    "rbush": "^2.0.1",
    "rbush-knn": "^2.1.0",
    "register-service-worker": "^1.6.2",
    "sharedLib": "^0.1.0",
    "tslib": "^1.9.3",
    "vue": "^2.6.10",
    "vue-awesome": "^3.4.0",
    "vue-class-component": "^7.0.1",
    "vue-property-decorator": "^8.0.0",
    "vue-router": "^3.0.2",
    "vuetify": "^1.5.7",
    "vuex": "^3.1.0",
    "vuex-class": "^0.3.2",
    "watrmarksJs": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/theming": "^5.0.3",
    "@storybook/vue": "^5.0.3",
    "@types/auth0-js": "^8.11.11",
    "@types/chai": "^4.1.7",
    "@types/d3": "^4.13.1",
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "^24.0.11",
    "@types/lunr": "^0.1.0",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-links": "^3.3.4",
    "@types/storybook__vue": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-pwa": "^3.5.1",
    "@vue/cli-plugin-typescript": "^3.5.1",
    "@vue/cli-plugin-unit-mocha": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.1.2",
    "eslint": "^5.15.3",
    "file-system": "^2.2.2",
    "fs-extra": "^7.0.1",
    "jest": "^24.5.0",
    "json-server": "^0.14.2",
    "material-design-icons-iconfont": "^4.0.5",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "server": "^0.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.3.4000",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.1",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "@": "<rootDir>/src"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": {
          "ignoreCodes": [
            151001,
            7016
          ]
        }
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "main": "./src/main.ts"
}
