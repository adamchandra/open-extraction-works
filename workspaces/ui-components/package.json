    "axios": "^0.19.2",
    "chroma-js": "^2.1.0",
    "d3": "^5.15.0",
    "d3-selection": "^1.4.1",
    "d3-transition": "^1.3.2",
    "eslint-plugin-nuxt": "^0.5.1",
    "json-server": "^0.15.1",
    "lunr": "^2.3.8",
