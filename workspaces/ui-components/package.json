    "@types/animejs": "^3.1.0",
    "@types/chai": "^4.2.9",
    "@types/chroma-js": "^1.4.3",
    "@types/d3-selection": "^1.4.1",
    "@types/d3-transition": "^1.1.6",
    "@types/jest": "^25.1.3",
    "@types/lunr": "^2.3.2",
