{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "description": "Backend server for WatrColors",
  "keywords": [
    "PDF",
    "labeling",
    "annotation",
    "text",
    "extraction"
  ],
  "author": "Adam Saunders <saunders@cs.umass.edu>",
  "homepage": "https://github.com/iesl/watr-jslibs#readme",
  "dependencies": {
    "commander": "^4.0.1",
    "fs-extra": "^8.1.0",
    "klaw": "^3.0.0",
    "klaw-sync": "^6.0.0",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-ejs": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "nodemon": "^2.0.1",
    "tslib": "^1.10.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.1.3",
    "@types/koa": "^2.11.2",
    "@types/koa-json": "^2.0.18",
    "@types/koa-router": "^7.4.0",
    "@types/koa-send": "^4.1.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "license": "ISC",
  "main": "./src/server.ts",
  "directories": {
    "src": "src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iesl/watr-jslibs.git"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "serve": "nodemon -w dist dist/server.js",
    "lint": "eslint --config .eslintrc.js --ext .ts --ignore-path ../../.gitignore ./src",
    "test": "jest",
    "setup": "yarn link commons && npm install"
  },
  "bugs": {
    "url": "https://github.com/iesl/watr-jslibs/issues"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/src/$1"
    }
  }
}
