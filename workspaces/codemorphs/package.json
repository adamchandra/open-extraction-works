{
  "name": "codemorphs",
  "version": "0.1.0",
  "private": true,
  "description": "Utilities to generate and manipulate typescript code",
  "keywords": [],
  "author": "Adam Saunders <saunders@cs.umass.edu>",
  "license": "ISC",
  "main": "./dist/main.js",
  "directories": {
    "src": "src"
  },
  "dependencies": {
    "commander": "^4.0.1",
    "fs-extra": "^8.1.0",
    "ts-morph": "^5.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.23"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "morph": "node dist/main.js",
    "mainxx": "npm-run-all --serial build \"morph -- {@}\" --",
    "main": "yarn build && yarn morph",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/iesl/watr-jslibs/issues"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/src/$1"
    }
  }
}
