{
  "name": "WatrFront",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "fabric": "^2.7.0",
    "jquery": "^3.4.1",
    "jquery-serializejson": "^2.9.0",
    "js-cookie": "^2.2.1",
    "jwt-client": "^0.2.1",
    "keyboardjs": "^2.5.1",
    "lunr": "^2.3.8",
    "query-string": "^6.9.0",
    "rbush": "^3.0.1",
    "register-service-worker": "^1.6.2",
    "requirejs": "^2.3.6",
    "rxjs": "^6.5.3",
    "simple-ajax-uploader": "^2.6.7",
    "split.js": "^1.5.11",
    "tippy.js": "^5.1.2",
    "vue": "^2.6.10",
    "vuex": "^3.1.2"
  },
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --log-level=debug",
    "build-v": "webpack --display-modules",
    "watch": "webpack --mode development --watch --log-level=debug",
    "start": "webpack-dev-server --mode development",
    "server": "node src/dev/dev-server.js",
    "test": "jest"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
