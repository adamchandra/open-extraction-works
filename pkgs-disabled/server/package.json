{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "description": "Backend server for WatrColors",
  "keywords": [
    "PDF",
    "labeling",
    "annotation",
    "text",
    "extraction"
  ],
  "author": "Adam Saunders <saunders@cs.umass.edu>",
  "homepage": "https://github.com/iesl/watr-jslibs#readme",
  "dependencies": {
    "commander": "^4.0.1",
    "fs-extra": "^8.1.0",
    "klaw": "^3.0.0",
    "klaw-sync": "^6.0.0",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-ejs": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "nodemon": "^2.0.1",
    "tslib": "^1.10.0"
  },
  "license": "ISC",
  "main": "./src/server.ts",
  "directories": {
    "src": "src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iesl/watr-jslibs.git"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "serve": "nodemon -w dist dist/server.js",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/iesl/watr-jslibs/issues"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": {
          "ignoreCodes": [
            151001,
            7016
          ]
        }
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
