{
    "allowJs" : true,
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "checkJs": true,
        "lib": [ "dom", "es2017" ]
        "module": "system",
        "noEmit": true,
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true
        "target": "es2017",
        "types" : ["node", "lodash", "express", "underscore"]
    },
    "include": [
        "vtrace/**/*"
    ],
    "exclude": [
    ]

}

